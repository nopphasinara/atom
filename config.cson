    restorePreviousWindowsOnStart: "always"
    simpleFullScreenWindows: true
    telemetryConsent: "no"
    themes: [
      "one-dark-ui"
      "one-dark-vivid-syntax"
    ]
    titleBar: "custom"
  editor:
    fontFamily: "Menlo, Prompt, MaterialDesignIcons, master-icons, monospace"
    maxScreenLineLength: 4096
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
    tabType: "soft"
  "exception-reporting":
    userId: "76b45aae-d46b-47bd-b54c-7d2b594202dd"
  hyperclick:
    darwinTriggerKeys: "altKey"
    linuxTriggerKeys: "altKey"
    win32TriggerKeys: "altKey"
  "linter-ui-default":
    largeFileLineCount: 50000
    longLineLength: 20000
    panelHeight: 300
  "markdown-preview-enhanced":
    openPreviewPaneAutomatically: false
  "one-dark-ui":
    hideDockButtons: true
  "project-manager":
    sortBy: "title"
  "remote-sync":
    autoHideLogPanel: true
    foldLogPanel: true
  "sass-autocompile":
    compileOnSave: false
    showStartCompilingNotification: true
  tabs:
    displayMruTabList: false
    enableMruTabSwitching: false
  welcome:
    showOnStartup: false
  whitespace:
    ensureSingleTrailingNewline: false
".blade.html.php.text":
  editor:
    commentEnd: " --}}"
    commentStart: "{{-- "
".html.php.text":
  editor:
    nonWordCharacters: "/\\()\"':,.;<>~!@#%^&*|+=[]{}`?-"
